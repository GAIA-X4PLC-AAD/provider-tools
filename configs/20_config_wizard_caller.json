	"data folder" : "metadata",
	"asset types" :	[
		{ "extension" : "xodr" }, 
		{ "extension" : "xosc" }
	],
	"params" : 	{
		"call" : "wizard-caller/main.py",
		"input" : {"" : "{path}/{sub_path}/domainMetadata.json"},
		"output" : {"-out" : "{path}/{sub_path}/domainMetadata.json"},
		"additional" : {
			"-shacl" : "{path}/temp/domainMetadata.ttl"
		}	
	}
}